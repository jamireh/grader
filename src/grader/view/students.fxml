               <padding>
                  <Insets bottom="10.0" top="10.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefWidth="200.0">
               <children>
                  <TextField fx:id="tfFirst" minHeight="-Infinity" minWidth="-Infinity" prefWidth="110.0" promptText="First">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <HBox.margin>
                        <Insets left="20.0" right="45.0" />
                     </HBox.margin>
                  </TextField>
                  <TextField fx:id="tfMiddle" minHeight="-Infinity" minWidth="-Infinity" prefWidth="110.0" promptText="Middle">
